alias ll='ls -lah'
alias ga='git add'
alias gc='git commit'
alias gp='git push'
alias vim="nvim"
alias oldvim="\vim"
alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index
export WORK="$HOME/Documents/work"
alias w='cd $WORK'
alias ui="tmuxp load ui-components -y"
alias acp="tmuxp load acp -y"
alias suitup="cd $WORK/suite-nonsplit && docker/sdk up --build --assets --data"
alias suitbuild="cd $WORK/suite-nonsplit && docker/sdk up --build --assets"
alias suitclean="cd $WORK/suite-nonsplit && docker/sdk stop && docker/sdk clean-data"
alias suitstop="cd $WORK/suite-nonsplit && docker/sdk stop"
